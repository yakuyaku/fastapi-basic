# Environment
ENVIRONMENT=development

# Database - Development
DB_HOST=localhost
DB_PORT=3306
DB_USER=fastapi_user
DB_PASSWORD=fastapi_password
DB_NAME=fastapi_db

# JWT - Development
SECRET_KEY=dev-secret-key-change-this-12345
